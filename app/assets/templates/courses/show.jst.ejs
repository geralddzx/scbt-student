<h2><%= course.get('name') %></h2>

<table border="0" cellspacing="5" cellpadding="5">  
  <tr>
    <td>Code</td>
    <td><%= course.get('code') %></td>
  </tr>
  
  <tr>
    <td>Start Date</td>
    <td><%= course.get('start_date') %></td>
  </tr>
  
  <tr>
    <td>End Date</td>
    <td><%= course.get('end_date') %></td>
  </tr>
  
  <tr>
    <td>Hours</td>
    <td><%= course.get('hours') %></td>
  </tr>
  
  <tr>
    <td>Instructor</td>
    <td>
      <% if (course.instructor){ %>
        <%= course.instructor.get('first_name') %> <%= course.instructor.get('last_name') %>
      <%}%>
    </td>
  </tr>
</table>
<%= JST["courses/_show_" + Scbt.Models.user.permissionType()]({course: course}) %>
  